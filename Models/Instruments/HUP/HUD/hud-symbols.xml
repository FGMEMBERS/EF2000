 <animation>
  <type>select</type>
  <object-name>autopilot-airspeed</object-name>
  <object-name>autopilot-airspeed-label</object-name>
  <condition>
	<equals>
	 <property>/systems/autopilot/settings/kts-mach-select</property>
	 <value>kts</value>
	</equals>
	<equals>
	  <property>/systems/autopilot/locks/speed</property>
	  <value>speed-with-throttle</value>
     </equals>
  </condition>
 </animation>
 
 <animation>
  <type>select</type>
  <object-name>autopilot-mach</object-name>
  <object-name>autopilot-mach-label</object-name>
  <condition>
    <equals>
	 <property>/systems/autopilot/locks/speed</property>
	 <value>speed-with-throttle</value>
    </equals>
	<equals>
	 <property>/systems/autopilot/settings/kts-mach-select</property>
	 <value>mach</value>
	</equals>	 
  </condition>
 </animation>
 
 <animation>
  <type>select</type>
  <object-name>autopilot-heading</object-name>
  <object-name>autopilot-heading-label</object-name>
  <condition>
     <equals>
	  <property>/systems/autopilot/locks/heading</property>
	  <value>heading-hold</value>
     </equals>
  </condition>
 </animation>
 
 <animation>
  <type>select</type>
  <object-name>autopilot-roll</object-name>
  <object-name>autopilot-roll-label</object-name>
  <condition>
     <equals>
	  <property>/systems/autopilot/locks/heading</property>
	  <value>roll-hold</value>
     </equals>
  </condition>
 </animation>
 
 <animation>
  <type>select</type>
  <object-name>autopilot-tacan</object-name>
  <object-name>autopilot-tacan-id</object-name>
  <condition>
   <equals>
    <property>/systems/autopilot/locks/heading</property>
    <value>nav-hold</value>
   </equals>
   <equals>
	<property>/computers/navigation/mode</property>
	<value>tacan</value>
   </equals>
  </condition>
 </animation>
 
 <animation>
  <type>select</type>
  <object-name>autopilot-nav</object-name>
  <condition>
   <and>
   <equals>
	<property>/systems/autopilot/locks/heading</property>
	<value>nav-hold</value>
   </equals>
    <equals>
	<property>/computers/navigation/mode</property>
	<value>nav</value>
   </equals>
   <equals>
	<property>/autopilot/route-manager/active</property>
	<value>true</value>
   </equals>
  </and>
  </condition>
 </animation>
 
 <animation>
