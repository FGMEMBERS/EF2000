 <animation>
  <type>translate</type>
  <object-name>ClimbDive.vv</object-name>
  <property>/orientation/alpha-deg</property>
  <factor>0.00495</factor>
  <condition>
   <greater-than>
    <property>/orientation/alpha-deg</property>
	<value>-5</value>
   </greater-than>
   <less-than>
    <property>/orientation/alpha-deg</property>
	<value>15</value>
   </less-than>
  </condition>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>-1</z>
  </axis>
 </animation>
 
 <!-- <animation>
  <type>textranslate</type>
  <object-name>Ladder</object-name>
  <property>/local/temp/hud-calibrate-x</property>
  <factor>10</factor>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation> -->
 
  <animation>
   <type>texmultiple</type>
   <object-name>Ladder</object-name>
   <transform>
     <property>/orientation/pitch-deg</property>
     <subtype>textranslate</subtype>
   <factor>0.0054</factor>
   <axis>
     <x>0</x>
     <y>1</y>
     <z>0</z>
   </axis>
   </transform>
  </animation>
  
  <animation>
   <type>texmultiple</type>
   <object-name>Ribbon.mag</object-name>
   <transform>
     <property>/orientation/heading-magnetic-deg</property>
     <subtype>textranslate</subtype>
   <factor>0.002795</factor>
   <axis>
     <x>0</x>
     <y>-1</y>
     <z>0</z>
   </axis>
   </transform>
  </animation>
  
  <animation>
   <type>texmultiple</type>
   <object-name>Ribbon.true</object-name>
   <transform>
     <property>/orientation/heading-deg</property>
     <subtype>textranslate</subtype>
   <factor>0.002795</factor>
   <axis>
     <x>0</x>
     <y>-1</y>
     <z>0</z>
   </axis>
   </transform>
  </animation>
 
 <animation>
  <object-name>Ladder</object-name>
  <object-name>Horizon</object-name>
  <type>rotate</type>
  <property>/orientation/roll-deg</property>
  <factor>1</factor>
  <offset-deg>0</offset-deg>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <object-name>BankRollPointer</object-name>
  <type>rotate</type>
  <property>/orientation/roll-deg</property>
  <factor>0.85</factor>
  <offset-deg>0</offset-deg>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <object-name>BankRollPointer</object-name>
  <type>select</type>
  <condition>
   <greater-than>
    <property>/orientation/roll-deg</property>
	<value>-60</value>
   </greater-than>
   <less-than>
    <property>/orientation/roll-deg</property>
	<value>60</value>
   </less-than>
  </condition>
 </animation>
 
 <animation>
  <object-name>AltDialPointer</object-name>
  <type>rotate</type>
  <property>/position/altitude-ft</property>
  <factor>0.36</factor>
  <offset-deg>0</offset-deg>
  <center>
   <x-m>0.006828</x-m>
   <y-m>0.05741</y-m>
   <z-m>0.0175</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <object-name>ClimbDive.lock</object-name>
  <type>select</type>
  <condition>
   <property>/instrumentation/HUD/display-locked</property>
  </condition>
 </animation>
 
 <animation>
  <object-name>ClimbDive.vv</object-name>
  <type>select</type>
  <condition>
   <not>
    <property>/instrumentation/HUD/display-locked</property>
   </not>
  </condition>
 </animation>
 
  <animation>
  <type>select</type>
  <object-name>mach-number</object-name>
  <object-name>mach-label</object-name>
  <condition>
   <and>
    <greater-than>
	 <property>/systems/electrical/outputs/avionics/HUD</property>
	 <value>27</value>
    </greater-than>
	<equals>
	 <property>/instrumentation/HUD/serviceable</property>
	 <value>true</value>
    </equals>
	<equals>
	 <property>/instrumentation/HUD/display-groundspeed</property>
	 <value>false</value>
    </equals>
   </and>
  </condition>
 </animation>
 
 <animation>
