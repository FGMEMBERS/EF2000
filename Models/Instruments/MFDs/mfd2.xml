 <animation>
  <type>pick</type>
  <object-name>power</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-toggle</command>
    <property>/instrumentation/MFD[2]/stand-by</property>    
   </binding>
  </action>
 </animation>
 
 <animation>
  <type>translate</type>
  <object-name>power</object-name>
  <property>/instrumentation/MFD[2]/stand-by</property>
  <factor>0.0075</factor>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

<animation>
 <type>material</type>
 <object-name>eng-page</object-name>
 <ambient>
   <red>0.0</red>
   <green>0.0</green>
   <blue>0.0</blue>
  </ambient>
  <diffuse>
   <red>0.0</red>
   <green>0.0</green>
   <blue>0.0</blue>
  </diffuse>
  <emission>
    <red>0.8</red>
    <green>0.8</green>
    <blue>0.8</blue>
    <factor-prop>/instrumentation/illumination/LCD</factor-prop>
  </emission>
 </animation>

</PropertyList>