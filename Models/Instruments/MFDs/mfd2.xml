<?xml version="1.0" encoding="utf-8"?>



<PropertyList>

 <path>mfd.ac</path>
 
 <model>
 <name>mfd-buttons-off</name>
  	<path>Aircraft/EF2000/Models/Instruments/MFDs/buttons-off.xml</path>
 </model>
 
 <animation>
  <type>select</type>
  <object-name>mfd-buttons-off</object-name>
  <condition>
   <or>
    <less-than>
     <property>/systems/electrical/outputs/avionics/MFHDD[2]</property>
     <value>26</value>
    </less-than>
    <equals>
     <property>/instrumentation/MFD[2]/serviceable</property>
	 <value>false</value>
    </equals>
	<equals>
    <property>/instrumentation/MFD[2]/stand-by</property>
    <value>true</value>
   </equals>
   </or>
  </condition>
 </animation>
 
 <model>
 <name>elev-prof-page</name>
 <path>Aircraft/EF2000/Models/Instruments/MFDs/ELEV/elev-prof.xml</path>
 <offsets>
	   <x-m>0.0</x-m>
	   <y-m>-0.000</y-m>
       <z-m>0.00</z-m>
	</offsets>
 </model>
 
 <model>
 <name>elev-buttons</name>
 <path>Aircraft/EF2000/Models/Instruments/MFDs/ELEV/elev-buttons.xml</path>
 </model>
 
 <animation>
  <type>select</type>
  <object-name>elev-prof-page</object-name>
  <condition>
   <greater-than>
    <property>/systems/electrical/outputs/avionics/MFHDD[2]</property>
    <value>26</value>
   </greater-than>
   <equals>
    <property>/instrumentation/MFD[2]/serviceable</property>
    <value>true</value>
   </equals>
   <equals>
    <property>/instrumentation/MFD[2]/stand-by</property>
    <value>false</value>
   </equals>
   <equals>
    <property>/instrumentation/MFD[2]/page-selected</property>
    <value>elev</value>
   </equals>
   <equals>
    <property>/instrumentation/MFD[2]/ELEV/c-scope</property>
    <value>false</value>
   </equals>
  </condition>
 </animation>
 
 <animation>
  <type>select</type>
  <object-name>elev-buttons</object-name>
  <condition>
   <greater-than>
    <property>/systems/electrical/outputs/avionics/MFHDD[2]</property>
