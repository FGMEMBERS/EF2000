<?xml version="1.0" encoding="utf-8"?>


<PropertyList>

 <path>stick.ac</path>
 
  <animation>
  <type>rotate</type>
  <object-name>Handle</object-name>
  <property>/controls/flight/elevator</property>
  <factor>3</factor>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>
 
   <animation>
  <type>rotate</type>
  <object-name>Handle</object-name>
  <property>/controls/flight/aileron</property>
