
<PropertyList>

 <!-- Pilot's Display String -->
 
 <path>identification.ac</path>
 
 <text>
  <name>pilotlabel-left</name>
  <font>condensed.txf</font>
  <character-size type="double">0.045</character-size>
  <offsets>
   <x-m>4.2</x-m>
   <y-m>-0.625</y-m>
   <z-m>0.57</z-m>
   <roll-deg>-35</roll-deg>
   <heading-deg>-4.25</heading-deg>
   <pitch-deg>-3</pitch-deg>
  </offsets>
 <axis-alignment>xz-plane</axis-alignment>
  <type type="string">text-value</type>
  <property>sim/multiplay/generic/string[0]</property>
  <draw-text type="bool">true</draw-text>
  <alignment>center-center</alignment>
 </text>
 
 <text>
  <name>pilotlabel-right</name>
  <font>condensed.txf</font>
