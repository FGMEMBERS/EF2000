    <object-name>landing.beam.left</object-name>
    <condition>
      <equals>
        <property>gear/gear[1]/position-norm</property>
        <value>1</value>
      </equals>
	  <greater-than>
	    <property>sim/multiplay/generic/float[4]</property>
		<value>100</value>
	  </greater-than>
    </condition>
  </animation>
  
  <animation>
