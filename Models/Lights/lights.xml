   <animation>
    <type>select</type>
    <object-name>TailLight</object-name>
    <object-name>LeftLight</object-name>
    <object-name>RightLight</object-name>
    <condition>
	 <greater-than>
      <property>sim/multiplay/generic/float[2]</property>
      <value>12</value>
	 </greater-than>
    </condition>
   </animation>
   
   <animation>
    <type>select</type>
    <object-name>UpperStrobe</object-name>
    <condition>
	 <and>
      <property>sim/multiplay/generic/float[3]</property>
	  <property>sim/model/EF2000/lights/strobe-upper/state</property>
	 </and>
    </condition>
   </animation>
   
   <animation>
    <type>select</type>
    <object-name>strobe-upper-point</object-name>
    <condition>
	 <and>
      <property>sim/multiplay/generic/float[3]</property>
	  <property>sim/model/EF2000/lights/strobe-upper/state</property>
	  <property>/sim/rendering/rembrandt/enabled</property>
	 </and>
    </condition>
   </animation>
   
   <animation>
    <type>select</type>
    <object-name>LowerStrobe</object-name>
    <condition>
      <property>sim/multiplay/generic/float[3]</property>
	  <property>sim/model/EF2000/lights/strobe-lower/state</property>
    </condition>
   </animation>
   
   <animation>
    <type>select</type>
    <object-name>cockpitlamp</object-name>
    <object-name>switchpanel.left</object-name>
    <object-name>switchpanel.right</object-name>
    <condition>
	 <and>
	   <greater-than>
       <property>systems/electrical/outputs/avionics/cockpit-lighting</property>
       <value>24</value>
	  </greater-than>
	  </and>
    </condition>
   </animation>

