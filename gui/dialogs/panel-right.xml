<?xml version="1.0" encoding="utf-8"?>

<PropertyList>

 <name>panel-right</name>
 <x>-50</x>
 <width>400</width>
 <height>700</height>
 <modal>false</modal>
 <draggable>false</draggable>
 <resizable>false</resizable>


 <group>
  <layout>hbox</layout>
  <x>200</x>
  <y>335</y>
  
  <button>
   <x>180</x>
   <width>12</width>
   <height>12</height>
   <legend></legend>
   <binding>
    <command>dialog-close</command>
   </binding>
  </button>
  
 </group>

 
 <frame>
  <layout>hbox</layout>
  <x>0</x>
  <y>560</y>
  <height>110</height>
  
  <group>
   <layout>vbox</layout>
   <x>40</x>
   <y>20</y>
  
   <button>
    <width>60</width>
    <height>60</height>
	<x>10</x>
	<y>10</y>
	<legend></legend>
	<live>true</live>
    <one-shot>false</one-shot>
    <binding>
     <command>property-toggle</command>
	 <property>/controls/buttons/external-lights-master</property>
    </binding>
   </button>
  
   <text>
    <x>0</x>
	<y>-15</y>
    <label>EXT LIGHTS</label>
   </text>
   
   </group>
  
   <group>
   <layout>vbox</layout>
   <y>20</y>
   <x>120</x>
   
  
   <checkbox>
    <width>30</width>
    <height>30</height>
	<x>25</x>
	<y>25</y>
	<live>true</live>
	<binding>
	 <command>property-toggle</command>
	 <property>/controls/switches/navigation-lights</property>
	</binding>
	<binding>
	 <command>dialog-update</command>
	</binding>
   </checkbox>
  
   <text>
    <x>20</x>
	<y>-15</y>
    <label>NAV</label>
   </text>
  
  </group>
  
  <group>
   <layout>vbox</layout>
   <y>20</y>
   <x>180</x>
   
  
   <checkbox>
    <width>30</width>
    <height>30</height>
	<x>25</x>
	<y>25</y>
	<property>/controls/switches/strobe-lights</property>
   </checkbox>
  
   <text>
    <x>10</x>
	<y>-15</y>
    <label>STROBE</label>
   </text>
  
  </group>
  
  <group>
   <layout>vbox</layout>
   <y>20</y>
   <x>240</x>
   
  
  <dial>
   <x>25</x>
   <y>25</y>
   <width>30</width>
   <property>/controls/rotary/formation-lights</property>
   <min>0</min>
   <max>1</max>
  </dial>
  
   <text>
    <x>15</x>
	<y>-15</y>
    <label>FORM</label>
   </text>
  
  </group>
  
  <group>
   <layout>vbox</layout>
   <y>20</y>
   <x>300</x>
   
  
   <checkbox>
    <width>30</width>
    <height>30</height>
	<x>25</x>
	<y>25</y>
	<property>/controls/switches/landing-lights</property>
   </checkbox>
  
   <text>
    <x>15</x>
	<y>-15</y>
    <label>GEAR</label>
   </text>
  
  </group>
  
 </frame>
 

  
  

