 <canopy-jettison>
  <name>Canopy Jettison</name>
  <mode>once</mode>
  <path>FX/canopy-jettison.wav</path>
  <condition>
    <property>/eject/canopy/jettison</property>
  </condition>
   <volume>
    <property>/sim/signals/fdm-initialized</property>
    <factor>1</factor>
    <offset>0</offset>
    <min>1.0</min>
	<max>1.0</max>
   </volume>
  </canopy-jettison>
  
  <!-- Voice -->
  
  <voice-low-height>
  <name>Low Height</name>
  <mode>once</mode>
  <path>Voice/lowheight.wav</path>
  <type>avionics</type>
  <condition>
    <property>/controls/switches/voice</property>
	<property>/systems/warnings/alerts/low-height/voice-trigger</property>
	<not><property>/systems/warnings/alerts/low-height/acknowledged</property></not>
  </condition>
   <volume>
    <property>/controls/rotary/voice-volume</property>
    <factor>0.5</factor>
    <offset>0</offset>
    <min>0.0</min>
	<max>1.0</max>
   </volume>
  </voice-low-height>
  
 <voice-speed-low>
  <name>Speed Low</name>
  <mode>once</mode>
  <path>Voice/speedlow.wav</path>
  <type>avionics</type>
  <condition>
    <property>/controls/switches/voice</property>
    <property>/systems/warnings/alerts/speed-low/voice-trigger</property>
	<not><property>/systems/warnings/alerts/low-height/acknowledged</property></not>
  </condition>
   <volume>
    <property>/controls/rotary/voice-volume</property>
    <factor>0.5</factor>
    <offset>0</offset>
    <min>0.0</min>
	<max>1.0</max>
   </volume>
  </voice-speed-low>
  
 <voice-mass-not-live>
  <name>Mass Not Live</name>
  <mode>once</mode>
  <path>Voice/massnotlive.wav</path>
  <type>avionics</type>
  <condition>
    <property>/controls/switches/voice</property>
    <property>/systems/warnings/alerts/mass-not-live/voice-trigger</property>
	<not><property>/systems/warnings/alerts/mass-not-live/acknowledged</property></not>
  </condition>
   <volume>
    <property>/controls/rotary/voice-volume</property>
    <factor>0.5</factor>
    <offset>0</offset>
    <min>0.0</min>
	<max>1.0</max>
   </volume>
  </voice-mass-not-live>
  
  <voice-landing-gear>
  <name>Landing Gear</name>
  <mode>once</mode>
  <path>Voice/landinggear.wav</path>
  <type>avionics</type>
  <condition>
    <property>/controls/switches/voice</property>
    <property>/systems/warnings/alerts/landing-gear/voice-trigger</property>
	<not><property>/systems/warnings/alerts/landing-gear/acknowledged</property></not>
  </condition>
   <volume>
    <property>/controls/rotary/voice-volume</property>
    <factor>0.5</factor>
    <offset>0</offset>
    <min>0.0</min>
	<max>1.0</max>
   </volume>
  </voice-landing-gear>
  
 </fx>
 
</PropertyList>